{
  "name": "tap-given",
  "version": "0.4.0",
  "description": "Given-When-Then BDD for TAP",
  "main": "lib/tap-given.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dex4er/js-tap-given"
  },
  "keywords": [
    "bdd",
    "cucumber",
    "gherkin",
    "given",
    "given-when-then",
    "tap"
  ],
  "author": "Piotr Roszatycki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dex4er/js-tap-given/issues"
  },
  "homepage": "http://github.com/dex4er/js-tap-given",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "nyc": "^10.2.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.1",
    "tap": "^10.3.2"
  },
  "scripts": {
    "test": "standard --verbose | snazzy && tap test/*.js",
    "test-coverage": "npm test -- --coverage",
    "test-onchange-lcovonly": "npm run -s test-coverage -- -R min --coverage-report lcovonly; onchange '**/*.js' -- npm run -s test-coverage -- -R min --coverage-report lcovonly"
  },
  "files": [
    "lib/",
    "index.js"
  ],
  "standard": {
    "globals": [
      "After",
      "AfterEach",
      "And",
      "Before",
      "BeforeEach",
      "Feature",
      "Given",
      "Scenario",
      "Then",
      "When"
    ]
  },
  "nyc": {
    "exclude": []
  }
}
