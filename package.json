{
  "name": "tap-given",
  "version": "0.0.1",
  "description": "Given-When-Then BDD for TAP",
  "main": "lib/tap-given.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dex4er/js-tap-given"
  },
  "keywords": [
    "bdd",
    "cucumber",
    "gherkin",
    "given",
    "given-when-then",
    "tap"
  ],
  "author": "Piotr Roszatycki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dex4er/js-tap-given/issues"
  },
  "homepage": "http://github.com/dex4er/js-tap-given",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "tap": "^10.1.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "nyc": "^10.1.2",
    "standard": "^8.6.0"
  },
  "scripts": {
    "test": "tap --timeout 20 test/*.js",
    "test-coverage": "nyc tap --timeout 120 test/*.js",
    "test-standard": "standard",
    "test-standard-coverage": "npm run test-standard && npm run test-coverage"
  },
  "nyc": {
